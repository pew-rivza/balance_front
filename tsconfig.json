{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "typeRoots": ["node_modules/@types", "src/typings"],
    "paths": {
      "components": ["components/*"]
    }
  }
}